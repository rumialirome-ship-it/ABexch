{
  "name": "a-baba-exchange-frontend",
  "version": "1.0.0",
  "description": "Frontend for A-BABA Exchange",
  "private": true,
  "scripts": {
    "build": "esbuild index.tsx --bundle --outdir=dist --splitting --format=esm --loader:.tsx=tsx --define:process.env.NODE_ENV='\"production\"' --minify",
    "dev": "esbuild index.tsx --bundle --outdir=dist --splitting --format=esm --loader:.tsx=tsx --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.4",
    "typescript": "^5.4.5"
  }
}